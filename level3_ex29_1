#include <stdio.h>
int main(){

    int arr[10];
    float sum = 0;
    
    for (int i = 0; i < 10; i++){
        printf("Enter Student %d height: ", i+1);
        scanf("%d", &arr[i]);
        sum += arr[i];
    }
    
    int i = 0;

    while (i < 10){
        printf("student %d is %d\n", i+1,arr[i]);
        i ++;
    }
    
    
    float result = sum / 10.00;
    printf("The average of all students is %.2f", result) ;



    return 0;
}
