// Online C compiler to run C program online
#include <stdio.h>

int main(){

    char laststr = NULL,*p,str[50];
    p = str;
    scanf("%s", str);
    
    while (*p != '\0'){
        if(*p == laststr){
            p ++;
        }else{
            laststr = *p;
            printf("%c", *p);
            p++;
        }
    }
    
    return 0;
}
